{
  "name": "eslint-plugin-itgalaxy",
  "version": "67.0.0",
  "description": "itgalaxy org's ESLint rules and configs.",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin",
    "config",
    "itgalaxy",
    "javascript",
    "styleguide"
  ],
  "author": "Itgalaxy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/eslint-plugin-itgalaxy.git"
  },
  "homepage": "https://github.com/itgalaxy/eslint-plugin-itgalaxy",
  "bugs": "https://github.com/itgalaxy/eslint-plugin-itgalaxy/issues",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.9.0",
    "eslint": "^4.17.0",
    "eslint-ava-rule-tester": "^2.0.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-lodash": "^2.4.3",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-unicorn": "^4.0.1",
    "husky": "^0.14.1",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.0.1",
    "npmpub": "^3.1.0",
    "nyc": "^11.0.3",
    "prettier": "^1.5.2",
    "prop-types": "^15.5.8",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-itgalaxy": "^12.0.0"
  },
  "peerDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.17.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-lodash": "^2.4.3",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-unicorn": "^4.0.1",
    "prettier": "^1.5.2"
  },
  "scripts": {
    "precommit": "lint-staged",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint:eslint-config-prettier-check": "eslint --print-config ./print-config/index.js | eslint-config-prettier-check",
    "lint:check-rules": "eslint-find-rules --unused lib/config/all.js",
    "lint:eslint": "eslint --cache --report-unused-disable-directives . --ignore-path .gitignore",
    "lint:remark": "remark . -i .gitignore -f -q",
    "lint:prettier": "prettier --list-different '{lib,__tests__}/**/*.{js,mjs,jsx,md}' '*.{js,mjs,jsx,md}'",
    "lint": "npm-run-all -l -p lint:**",
    "prettify": "npm run lint:prettier -- --write",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "npm-run-all -l prettify -p 'fix:**'",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "nyc ava --verbose '**/__tests__/**/*.js'",
    "release": "npmpub"
  },
  "ava": {
    "require": "babel-register",
    "concurrency": 5,
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "sort-keys": "off"
    },
    "extends": [
      "./lib/config/ava.js",
      "./lib/config/node.js",
      "./lib/config/esnext.js"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/**/*.js"
        ],
        "parserOptions": {
          "sourceType": "module"
        },
        "rules": {
          "node/no-unsupported-features": "off"
        }
      }
    ],
    "root": true
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  },
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0"
  }
}
