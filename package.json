{
    "name": "eslint-plugin-itgalaxy",
    "version": "2.0.1",
    "description": "itgalaxy org's ESLint rules and configs.",
    "keywords": [
        "eslint",
        "eslintconfig",
        "eslintplugin",
        "eslint-config",
        "eslint-plugin",
        "config",
        "itgalaxy",
        "javascript",
        "styleguide"
    ],
    "author": "Itgalaxy",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/itgalaxy/eslint-plugin-itgalaxy.git"
    },
    "homepage": "https://github.com/itgalaxy/eslint-plugin-itgalaxy",
    "bugs": "https://github.com/itgalaxy/eslint-plugin-itgalaxy/issues",
    "main": "index.js",
    "files": [
        "lib",
        "index.js"
    ],
    "dependencies": {
        "eslint-plugin-ava": "~2.5.0",
        "eslint-plugin-import": "~1.8.0",
        "eslint-plugin-jsx-a11y": "~1.5.0",
        "eslint-plugin-lodash": "~1.9.0",
        "eslint-plugin-node": "~1.4.0",
        "eslint-plugin-objects": "~1.1.0",
        "eslint-plugin-promise": "~1.3.0",
        "eslint-plugin-react": "~5.1.0",
        "merge": "~1.2.0"
    },
    "devDependencies": {
        "eslint": "~2.12.0",
        "eslint-plugin-itgalaxy": "~2.0.0",
        "npmpub": "~3.1.0",
        "ava": "~0.15.0"
    },
    "peerDependencies": {
        "eslint": "~2.12.0"
    },
    "eslintConfig": {
        "rules": {
            "strict": "off"
        },
        "extends": [
            "plugin:itgalaxy/ava",
            "plugin:itgalaxy/es5",
            "plugin:itgalaxy/node"
        ]
    },
    "scripts": {
        "lint:eslint": "eslint . --ignore-path .gitignore",
        "ava": "ava \"__tests__/**/*.js\"",
        "test": "npm run lint:eslint && npm run ava",
        "release": "npmpub"
    }
}
