{
  "name": "eslint-plugin-itgalaxy",
  "version": "128.0.0",
  "description": "Itgalaxy org's ESLint rules and configs.",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin",
    "config",
    "itgalaxy",
    "javascript",
    "styleguide"
  ],
  "author": "Itgalaxy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/eslint-plugin-itgalaxy.git"
  },
  "homepage": "https://github.com/itgalaxy/eslint-plugin-itgalaxy",
  "bugs": "https://github.com/itgalaxy/eslint-plugin-itgalaxy/issues",
  "main": "index.js",
  "engines": {
    "node": ">= 10.13.0"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.9"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "ava": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-unicorn": "^21.0.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark-cli": "^8.0.1",
    "remark-preset-lint-itgalaxy": "^15.0.0",
    "standard-version": "^9.0.0"
  },
  "peerDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-unicorn": "^21.0.0"
  },
  "scripts": {
    "security": "npm audit",
    "lint:check-rules": "eslint-find-rules --unused lib/config/all.js",
    "lint:eslint-config-prettier-check": "eslint --print-config \"./__tests__/all-rules/index.jsx\" | eslint-config-prettier-check | true",
    "lint:prettier": "prettier . --list-different --ignore-path .gitignore",
    "lint:js": "eslint . --cache --ignore-pattern \"**/fixtures/**/*\" --ignore-path .gitignore --ext \".js,.cjs,.mjs,.jsx,.md\"",
    "lint:md": "remark . -i .gitignore -f -q",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "prettier": "npm run lint:prettier -- --write",
    "fix:js": "npm run lint:js -- --fix",
    "fix": "npm-run-all -c -l -p \"fix:**\" -s prettier",
    "pretest": "npm run lint",
    "test:only": "ava",
    "test:coverage": "nyc ava",
    "test": "npm run test:coverage",
    "release": "standard-version"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": true
      }
    },
    "concurrency": 5,
    "files": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  }
}
