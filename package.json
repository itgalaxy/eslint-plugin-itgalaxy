{
  "name": "eslint-plugin-itgalaxy",
  "version": "111.0.0",
  "description": "Itgalaxy org's ESLint rules and configs.",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslintplugin",
    "eslint-config",
    "eslint-plugin",
    "config",
    "itgalaxy",
    "javascript",
    "styleguide"
  ],
  "author": "Itgalaxy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/eslint-plugin-itgalaxy.git"
  },
  "homepage": "https://github.com/itgalaxy/eslint-plugin-itgalaxy",
  "bugs": "https://github.com/itgalaxy/eslint-plugin-itgalaxy/issues",
  "main": "index.js",
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.8"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-unicorn": "^11.0.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-itgalaxy": "^15.0.0",
    "standard-version": "^7.0.0"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-plugin-ava": "^8.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-unicorn": "^11.0.0"
  },
  "scripts": {
    "lint:check-rules": "eslint-find-rules --unused lib/config/all.js",
    "lint:eslint-config-prettier-check": "eslint --print-config \"./__tests__/all-rules/index.jsx\" | eslint-config-prettier-check | true",
    "lint:prettier": "prettier --list-different \"{lib,__tests__}/**/*.{js,mjs,jsx,md,yml,yaml}\" \"*.{js,mjs,jsx,md,yml,yaml}\"",
    "lint:js": "eslint . --cache --ignore-pattern \"**/fixtures/**/*\" --ignore-path .gitignore --ext \"js,.mjs,.jsx,.md\"",
    "lint:md": "remark . -i .gitignore -f -q",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "prettier": "npm run lint:prettier -- --write",
    "fix:js": "npm run lint:js -- --fix",
    "fix": "npm-run-all -l prettier -p \"fix:**\"",
    "pretest": "npm run lint",
    "test:only": "ava",
    "test:coverage": "nyc ava",
    "test": "npm run test:coverage",
    "release": "standard-version"
  },
  "ava": {
    "concurrency": 5,
    "files": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  }
}
